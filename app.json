{
  "expo": {
    "name": "LenHub",
    "slug": "lenhub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "lenhub",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "cover",
      "backgroundColor": "#E6E6E6"
    },
    "android": {
      "package": "com.lenhub.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "splash": {
        "backgroundColor": "#E6E6E6",
        "resizeMode": "cover"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-firebase-core",
        {
          "androidAppId": "1:617887325900:android:0f6627570410ba9e106c69"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "31e322c8-0b52-4d30-b93b-a9b2dff03f12"
      }
    },
    "platforms": ["android"]
  }
}
